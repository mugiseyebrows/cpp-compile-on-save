{
    "serialPort": "COM4",
    "editor": ["qtcreator", ["-client"]],
    "configEditor": ["Code", ["-r", "-g"]],
    "commands": {
        "shown": [{
            "name": "make",
            "task": true,
            "cmd": ["make", ["-j4"]]
        }, {
            "name": "clean",
            "task": true,
            "cmd": ["make", ["-j4", "clean"]]
        }],
        "hidden": [{
            "name": "edit",
            "cmd": ["qtcreator", ["-client", "$projectFile"]]
        }, {
            "name": "qmake",
            "task": true,
            "cmd": ["qmake"]
        }, {
            "name": "gitk",
            "cmd": ["gitk", ["--all"]]
        }, {
            "name": "bash",
            "cmd": ["bash"]
        }, {
            "name": "explore",
            "cmd": ["nautilus", ["$cwd"]]
        }]
    },
    "bookmarks": [{
        "name": "geany",
        "cmd": ["geany"]
    }, {
        "name": "vscode",
        "cmd": ["Code", ["-r", "-g"]]
    }, {
        "name": "qtcreator",
        "cmd": ["qtcreator"]
    }]
}